{
  "name": "arcreactor-web.react",
  "version": "1.0.0",
  "description": "React front-end app for arcreactor.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptainStiggz/arcreactor-web.react.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "rm public/index.html && cp src/index-dev.html public/index.html && NODE_ENV=development webpack-dev-server --host 127.0.0.1 --port 1337 --inline --hot",
    "build": "rimraf dist/ && NODE_ENV=production forever stopall && NODE_ENV=production webpack -p && babel ./express --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "NODE_ENV=production forever start dist/index.js",
    "build-start": "npm run build && npm run start",
    "stop": "forever stopall"
  },
  "author": "",
  "license": "ISC",
